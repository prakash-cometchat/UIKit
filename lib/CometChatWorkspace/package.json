{
  "name": "cometchat-pro-vue-ui-kit",
  "version": "3.0.2-beta1-1",
  "description": "CometChat UI Kit for Vue app.",
  "scripts": {
    "dev": "watch 'npm run develop' .",
    "develop": "sh develop.sh",
    "styleguide": "vue-cli-service styleguidist",
    "styleguide:build": "vue-cli-service styleguidist:build",
    "build": "rm -rf dist && NODE_ENV=production babel src/components --out-dir dist --copy-files"
  },
  "dependencies": {
    "@cometchat-pro/chat": "3.0.2-beta1",
    "dateformat": "^4.5.1",
    "twemoji": "^13.1.0"
  },
  "devDependencies": {
    "vue-cli-plugin-styleguidist": "~4.43.0",
    "vue-docgen-cli": "^4.43.0",
    "vue-styleguidist": "^4.43.0",
    "watch": "^1.0.2"
  }
}
